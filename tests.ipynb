    "from datetime import datetime\n",
    "\n",
    "# A date string\n",
    "date_str = \"2023-03-20\"\n",
    "\n",
    "# Define the format of the date string\n",
    "date_format = \"%Y-%m-%d\"\n",
    "\n",
    "# Convert the string to a date object\n",
    "date_obj = datetime.strptime(date_str, date_format).date()\n",
    "print(date_obj)"
   ]
